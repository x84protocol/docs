{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "x84 Protocol",
  "description": "The infrastructure layer for autonomous AI agents on Solana â€” on-chain identity, reputation, delegation, x402 payments, and managed hosting.",
  "colors": {
    "primary": "#FAFAFA",
