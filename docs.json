{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "x84 Protocol",
  "colors": {
    "primary": "#FAFAFA",
    "light": "#FAFAFA",
    "dark": "#27272A"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "background": {
    "decoration": "grid",
    "color": {
      "dark": "#09090B"
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://x84.dev"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "quickstart", "concepts"]
          },
          {
            "group": "Protocol",
            "pages": [
              "protocol/identity",
              "protocol/reputation",
              "protocol/delegation",
              "protocol/payments"
            ]
          },
          {
            "group": "Infrastructure",
            "pages": ["hosting", "mcp-server"]
          }
        ]
      },
      {
        "tab": "SDK",
        "groups": [
          {
            "group": "SDK",
            "pages": [
              "sdk/overview",
              "sdk/instructions",
              "sdk/types",
              "sdk/settlement"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "GitHub",
          "href": "https://github.com/x84-protocol",
          "icon": "github"
        },
        {
          "anchor": "Explorer",
          "href": "https://x84.dev/explorer",
          "icon": "compass"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Github",
        "href": "https://github.com/x84protocol"
      },
      {
        "label": "Follow on X",
        "href": "https://x.com/x84protocol"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch App",
      "href": "https://x84.ai/dashboard"
    }
  },
  "contextual": {
    "options": ["copy", "view", "claude", "chatgpt", "cursor", "mcp"]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/x84protocol",
      "github": "https://github.com/x84protocol"
    }
  }
}
